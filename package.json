{
  "name": "tiktok-creator-monetization-calculator",
  "version": "1.0.0",
  "description": "Professional earnings calculator for content creators across multiple platforms",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run i18n:all && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate && npm run i18n:types",
    "i18n:check": "i18n-toolkit check",
    "i18n:sync": "i18n-toolkit sync",
    "i18n:types": "i18n-toolkit types",
    "i18n:validate": "i18n-toolkit validate",
    "i18n:clean": "i18n-toolkit clean --interactive",
    "i18n:report": "i18n-toolkit report",
    "i18n:all": "i18n-toolkit check --verbose && i18n-toolkit sync && i18n-toolkit types",
    "i18n:enterprise:init": "node scripts/i18n-enterprise.js init",
    "i18n:enterprise:status": "node scripts/i18n-enterprise.js status",
    "i18n:enterprise:sync": "node scripts/i18n-enterprise.js sync",
    "i18n:enterprise:validate": "node scripts/i18n-enterprise.js validate",
    "i18n:enterprise:full": "node scripts/i18n-enterprise.js full",
    "clean": "rm -rf .next out dist build",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "i18n:extract": "i18n-toolkit check",
    "i18n:add": "i18n-toolkit sync --interactive",
    "i18n:check:verbose": "i18n-toolkit check --verbose",
    "i18n:check:dry": "i18n-toolkit check --dry-run",
    "i18n:check:ci": "i18n-toolkit validate",
    "i18n:dev": "i18n-toolkit sync --interactive",
    "i18n:smart": "i18n-toolkit check",
    "i18n:help": "i18n-toolkit --help",
    "i18n:validate-old": "npm run i18n:types && npm run i18n:validate",
    "pre-commit": "npm run i18n:check && npm run lint && npm run type-check"
  },
